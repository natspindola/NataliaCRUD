@model Natalia.Web.ViewModels.FabricanteViewModel

<h5>FABRICANTE E CATEGORIA</h5>
<br />
<div  class="row">
    <div class="col-md-5">
        <form enctype="multipart/form-data" asp-action="Create">
            <div class="form-group">
                <label class="control-label">FABRICANTE</label>
                <input asp-for="Nome" class="form-control" placeholder="Digite o nome do fabricante" />
                <span asp-validation-for="Nome" class="text-danger"></span>
            </div>
            <label class="control-label">Categoria</label>
            <div class="form-group">
                <input asp-for="Categoria1" class="form-control" placeholder="Digite o nome da categoria 1" />
            </div>
            <div class="form-group">
                <input asp-for="Categoria2" class="form-control" placeholder="Digite o nome da categoria 2" />
            </div>
            <div class="form-group">
                <input asp-for="Categoria3" class="form-control" placeholder="Digite o nome da categoria 3" />
            </div>            
            
            <div class="form-group">
                <input type="submit" value="Adicionar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>